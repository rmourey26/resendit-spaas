# Registry configuration
registry=https://registry.npmjs.org/

# Security settings
audit-level=moderate
fund=false

# Performance settings
prefer-offline=true
cache-max=86400000

# Package resolution
save-exact=true
package-lock=false

# Logging
loglevel=warn

# Node version compatibility
engine-strict=true

# Dependency resolution
legacy-peer-deps=false
strict-peer-deps=true

# Cache settings
cache-min=10
